<div class="all-forms">
<h4>Please Login</h4>
<br>
 <%= form_for :user, url: sessions_path do |f| %>
    <div class = "input-field">
      <%= f.label :username %>
      <%= f.text_field :username %>
    </div>
    <div class = "input-field">
    <%= f.label :password %>
    <%= f.password_field :password %>
  	</div>
 <%= f.submit "Login"%>
<% end %>
  <div class = "errors">
  <% if @errors %>
    <%= @errors %>
  <% end %>
  </div>
</div>